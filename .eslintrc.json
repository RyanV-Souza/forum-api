{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "off",
    "no-new": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}