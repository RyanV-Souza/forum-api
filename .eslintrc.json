{
  "extends": [
    "@rocketseat/eslint-config/node"
    // "plugin:vitest-globals/recommended"
  ],
  // "env": {
  //   "vitest-globals/env": true
  // },
  "rules": {
    "@typescript-eslint/ban-types": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-useless-constructor": "off",
    "camelcase": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}